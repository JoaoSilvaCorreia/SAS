#Anova Teste de Tukey

OPTIONS NONUMBER PS=60 NODATE;
DATA TESTE;
	INPUT TRATAMENTO $ PRODUCAO;
	LABEL TRATAMENTO = 'Variedades de milho'
		PRODUCAO = 'Produção em kg/100m2';
	cards;
	A 25 
	A 26 
	A 20 
	A 23 
	A 21
	B 31 
	B 25 
	B 28 
	B 27 
	B 24
	C 22 
	C 26 
	C 28 
	C 25 
	C 29
	D 33 
	D 29 
	D 31 
	D 34 
	D 28
	;
	
	PROC MEANS CSS;
	VAR TRATAMENTO;
	RUN; 
	
	PROC ANOVA;
	CLASS TRATAMENTO; /*VARIAVEL OBSERVADA */
	MODEL PRODUCAO = TRATAMENTO; /*AS VARIACOES NA PRODUCAO SAO CAUSADAS PELOS TIPOS DE TRATAMENTO*/
	RUN;  
	
	MEANS TRATAMENTO/TUKEY;
	MEANS TRATAMENTO/DUNNETT;
	RUN;
	
	
	
	
