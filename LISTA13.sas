/*exemplo */

DATA EMPRESA;
	INPUT RECEITA PROPTV PROPJO;
	CARDS;
	
	96 5.0 1.5
	90 2.0 2.0
	95 4.0 1.5
	92 2.5 2.5
	95 3.0 3.3
	94 3.5 2.3
	94 2.5 4.5
	94 3.0 2.5
	;
	

TITLE 'DIAGRAMA DE DISPERSÃO';
PROC PLOT;
	PLOT RECEITA*PROPTV;
	PLOT RECEITA*PROPJO;
RUN;


TITLE 'COEFICIENTE DE CORRELAÇÃO';
PROC CORR;
	VAR RECEITA PROPTV PROPJO;
RUN;

TITLE 'REGRESSÃO';
PROC REG;
	MODEL RECEITA = PROPTV PROPJO;
RUN; 
QUIT;

